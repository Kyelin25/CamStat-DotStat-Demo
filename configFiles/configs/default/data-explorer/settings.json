{
  "sdmx": {
    "datasourceIds": [
      "CamStat-stable"
    ],
    "range": [0, 2500],
    "attributes": {
      "flags": ["OBS_STATUS", "BASE_PER", "SOURCE_DETAIL"],
      "footnotes": ["TIME_FORMAT", "UNIT_MULT", "UNIT_MEASURE", "REF_PERIOD", "TRANSFORMATION", "VALUATION", "METHODOLOGY"],
      "prefscale": "PREF_SCALE", 
      "decimals": "DECIMALS"
    },
    "units": {
      "id": "UNIT",
      "annotationsDefinitionCodes": {
        "concepts": ["UNIT_MEASURE_CONCEPTS"]
      },
      "defaultCodes": ["UNIT_MEASURE", "UNIT_MULT", "BASE_PER"],
      "rejectedValueIds": ["_L", "_T", "_Z", "0"]
    },
    "period": {
      "boundaries": ["1970", "2020"],
      "default": ["1990", "2020"]
    },
    "frequency": "A"
  },
  "viewer": {
    "owner": "OECD",
    "logo": "http://localhost:5007/assets/siscc/data-explorer/images/sis-cc-logo.png",
    "terms": {
      "label": "Terms & Conditions",
      "link": "http://www.oecd.org/termsandconditions/"
    },
    "api": {
      "doc": "https://data.oecd.org/api/sdmx-json-documentation/",
      "contact": "https://stats.oecd.org/FAQAndContact.aspx"
    }
  },
  "app": {
    "title": "Example Data Explorer",
    "favicon": "http://localhost:5007/assets/siscc/data-explorer/images/favicon.ico"
  },
  "assets": {
    "header": "http://localhost:5007/assets/siscc/data-explorer/images/sis-cc-logo.png",
    "subheader": "http://localhost:5007/assets/siscc/data-explorer/images/dotstat-data-explorer-logo.png",
    "footer": "http://localhost:5007/assets/siscc/data-explorer/images/sis-cc-icon.png",
    "splash": "http://localhost:5007/assets/siscc/data-explorer/images/dotstat-data-explorer-logo.png"
  },
  "chart": {
    "url": "",
    "source": "http://dotstat.oecd.org/",
    "maps": {
      "fra-poc": {
        "id": "fra-poc",
        "projection": "mercator",
        "path": "/assets/siscc/data-explorer/maps/fra-poc.json",
        "levels": ["dep"],
        "scale": 2000
      }
    },
    "options": {
      "base": {
        "height": 700
      },
      "axis": {
        "x": {
          "font": {
            "family": "'Segoe UI'"
          }
        },
        "y": {
          "font": {
            "family": "'Segoe UI'"
          }
        }
      },
      "serie": {
        "annotation": {
          "font": {
            "family": "'Segoe UI'"
          }
        },
        "tooltip": {
          "font": {
            "family": "'Segoe UI'"
          }
        }
      }
    }
  },
  "table": {
    "cellsLimit": 6000
  },
  "theme": {
    "overrides": {
      "MuiButton": {
        "root": {
          "textTransform": "'none'"
        }
      }
    },
    "palette": {
      "action": {
        "active": "rgba(14, 144, 144, 0.54)",
        "selected": "rgba(14, 144, 224, 0.25)",
        "hover": "rgba(14, 144, 224, 0.13)"
      },
      "primary": {
        "main": "#0965c1",
        "light": "#0e90e0",
        "dark": "#0549ab"
      },
      "secondary": {
        "main": "#e3e9ed",
        "light": "#f5f8fa",
        "dark": "#ebf1f5"
      },
      "tertiary": {
        "light": "#e2f2fb",
        "dark": "#b7def6"
      },
      "button": {
        "mainOpacity": "rgba(0, 0, 0, 0.2)"
      },
      "highlight": {
        "hl1": "#f7a42c",
        "hl2": "#8CC841"
      },
      "grey": {
        "200": "#F3F7FB",
        "300": "#cccccc",
        "700": "#666666",
        "A700": "#182026"
      }
    },
    "mixins": {
      "dataflow": {
        "fontWeight": 400,
        "fontFamily": "'Roboto Slab', serif"
      },
      "scopeList": {
        "fontWeight": 700,
        "fontFamily": "'PT Sans Narrow', 'Helvetica Neue', Helvetica, Arial, sans-serif"
      },
      "excel": {
        "headerFont": "#ffffff",
        "sectionFont": "#000000",
        "rowFont": "#000000"
      },
      "sisccButton": {
        "&:hover": {
          "backgroundColor": "#f7a42c"
        }
      }
    },
    "typography": {
      "fontFamily": "'Helvetica Neue', Helvetica, Arial, sans-serif",
      "h6": {
        "fontSize": " 1.0625rem"
      }
    }
  },
  "i18n": {
    "localeId": "en",
    "locales": {
      "en": {
        "id": "en",
        "delimiters": { "thousands": ",", "decimal": "."  },
        "timeFormat": "YYYY-MMM"
      },
      "fr": {
        "id": "fr",
        "delimiters": { "thousands": " ", "decimal": ","  },
        "timeFormat": "MMM YYYY"
      }
    }
  },
  "ga": {},
  "styles": "http://localhost:5007/assets/default/data-explorer/styles/styles.css",
  "search": {
    "endpoint": "http://localhost:3004/api",
    "homeFacetIds": ["Topic"],
    "defaultRows": 10
  },
  "share": {
    "confirmUrl": "http://localhost:7002",
    "endpoint": "http://localhost:3005/api/charts",
    "policy": "http://www.oecd.org/general/privacypolicy.htm"
  }
}
